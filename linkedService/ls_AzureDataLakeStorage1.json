{
	"name": "ls_AzureDataLakeStorage1",
	"properties": {
		"parameters": {
			"adlsAccountName": {
				"type": "string"
			},
			"adlsSecretName": {
				"type": "string"
			},
			"keyVaultName": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "AzureBlobFS",
		"typeProperties": {
			"url": "@{concat('https://',linkedService().adlsAccountName,'.dfs.core.windows.net/')}",
			"accountKey": {
				"type": "AzureKeyVaultSecret",
				"store": {
					"referenceName": "ls_AzureKeyVault1",
					"type": "LinkedServiceReference",
					"parameters": {
						"keyVault": {
							"value": "@linkedService().keyVaultName",
							"type": "Expression"
						}
					}
				},
				"secretName": {
					"value": "@linkedService().adlsSecretName",
					"type": "Expression"
				}
			}
		}
	},
	"type": "Microsoft.DataFactory/factories/linkedservices"
}